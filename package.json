{
  "name": "vscode-webview-react",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.23.0"
  },
  "publisher": "rebornix",
  "activationEvents": [
    "onCommand:react-webview.start"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-webview.start",
        "title": "Start React Webview",
        "category": "React"
      }
    ]
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tachyons": "^4.12.0",
    "terser": "5.7.1",
    "vscode": "^1.1.37"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "scripts": {
    "vscode:prepublish": "node ./scripts/esbuild.js",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "build": "node ./scripts/esbuild.js",
    "watch": "node ./scripts/esbuild.js --watch",
    "start": "node ./scripts/esbuild.js --watch --expose"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@web/dev-server": "^0.1.18",
    "esbuild": "^0.12.15",
    "fs-extra": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}